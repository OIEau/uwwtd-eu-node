function uwwtdeu_schema()
{
  $schema['uwwtd_import_errors'] = array(
    'description' => 'Table for storing all import errors',
    'fields' => array(
      'errid' => array(
        'description' => 'The primary identifier for a error.',
        'type' => 'serial',
        'unsigned' => TRUE,
        'not null' => TRUE,
      ),
      'type' => array(
        'description' => 'Type of the error.',
        'type' => 'int',
        'not null' => FALSE,
        'default' => NULL,
      ),
      'category' => array(
        'description' => 'Category of the error',
        'type' => 'int',
        'not null' => FALSE,
        'default' => NULL,
      ),
      'year' => array(
        'description' => 'reference year of data',
        'type' => 'int',
        'not null' => false,
        'default' => NULL,
      ),
      'error' => array(
        'description' => 'description of the error.',
        'type' => 'text',
        'not null' => TRUE,
        'size' => 'medium',
      ),
      'date' => array(
        'description' => 'The Unix timestamp of the error.',
        'type' => 'int',
        'not null' => TRUE,
        'default' => 0,
      ),
      'entity_type' => array(
        'description' => 'The type of entity.',
        'type' => 'varchar',
        'length' => 32,
        'not null' => FALSE,
      ),
      'entity_id' => array(
        'description' => 'The primary identifier for an entity.',
        'type' => 'int',
        'unsigned' => TRUE,
        'not null' => FALSE,
      ),
      'bundle' => array(
        'description' => 'The bundle to which this entity belongs.',
        'type' => 'varchar',
        'length' => 128,
        'not null' => FALSE,
      ),
    ),
    'indexes' => array(
      'uwwtd_err_date'        => array('date'),
      'uwwtd_err_entity_id'        => array('entity_id'),
      'uwwtd_err_type'        => array('type'),
      'uwwtd_err_cat'        => array('category'),

    ),
    'primary key' => array('errid'),
  );
  return $schema;
}

/**
 * Implements hook_install().
 */
function uwwtdeu_install() {
  node_types_rebuild();
  _uwwtdeu_add_custom_fields();
}

/**
 * Implements hook_uninstall().
 */
function uwwtdeu_uninstall() {
  $nodeTypes = array('agglomeration', 'ms_level', 'nuts2_region');

  // Select and delete the nodes
  $sql = 'SELECT nid FROM {node} n WHERE n.type IN (:types)';
  $result = db_query($sql, array(':types' => $nodeTypes));
  $nids = array();
  foreach ($result as $row) {
    $nids[] = $row->nid;
  }
  node_delete_multiple($nids);

  // Delete the custom fields
  _uwwtdeu_delete_custom_fields();

  // Delete the content types
  foreach ($nodeTypes as $nodeType) {
    node_type_delete($nodeType);
  }

  // Purge the custom fields
  field_purge_batch(500);
}

function _uwwtdeu_add_custom_fields() {
  // Create the custom fields
  foreach (_uwwtdeu_field_bases() as $field) {
    field_create_field($field);
  }

  // Create the custom field instances
  foreach (_uwwtdeu_field_instances() as $fieldinstance) {
    $fieldinstance['entity_type'] = 'node';
    $fieldinstance['bundle'] = $fieldinstance['bundle'];
    field_create_instance($fieldinstance);
  }
}

function _uwwtdeu_delete_custom_fields() {
  // Delete the custom fields
  foreach (array_keys(_uwwtdeu_field_bases()) as $field) {
    field_delete_field($field);
  }

  // Delete the custom field instances
  $instances = field_info_instances('node', 'agglomeration');
  foreach ($instances as $instance_name => $fieldinstance) {
    field_delete_instance($fieldinstance);
  }
  $instances = field_info_instances('node', 'ms_level');
  foreach ($instances as $instance_name => $fieldinstance) {
    field_delete_instance($fieldinstance);
  }
  $instances = field_info_instances('node', 'nuts2_region');
  foreach ($instances as $instance_name => $fieldinstance) {
    field_delete_instance($fieldinstance);
  }
}

